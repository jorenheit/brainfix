include "std/io.bfx"    

function main()
{
    prints("f[0] = ");
    let a = scand();

    prints("f[1] = ");
    let b = scand();

    prints("Up to n = ");
    let n = scand();

    prints("0: "); printd(a); endl();
    for (let i = 0; i != n - 1; ++i)
    {
        printd(i + 1); prints(": ");
        printd(b); endl();
        
        let tmp = b; 
        b += a;
        a = tmp;
    }

    printd(i + 1); prints(": ");
    printd(b); endl();
}

